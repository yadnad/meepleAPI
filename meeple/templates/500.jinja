{% extends "base.jinja" %}

{% block title %}Server Error{% endblock %}
{% block body_id %}not-found{% endblock %}
{% block body_class %}not-found-page{% endblock %}
{% block content %}
    {% include "units/header.jinja" with header_class='navbar navbar-inverse normal' logo=True %}
    <div class="info">
        <p>Ooops!</p>
        <a href="https://www.youtube.com/watch?v=1u8HQNG9k2s" target="_blank">
            <img src="http://media.giphy.com/media/6IZzimIQN8guI/giphy.gif"/>
        </a>

        <p>Looks like something went wrong!</p>

        <p class="go-back">
            We track these errors automatically, but if the problem persists feel free to <a
                href="mailto:miha.zelnik@gmail.com">contact us</a>.
            In the meantime, try refreshing.
        </p>

    </div>
    <div id="container">
    </div>
{% endblock %}

